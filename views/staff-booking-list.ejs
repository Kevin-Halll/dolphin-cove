<section class="flex justify-center w-full">
    <section>
        <h1 class="text-3xl font-bold text-blue-900 text-center p-5 my-5 border-b-2 border-b-yellow-400 uppercase">
            Event Bookings
        </h1>
       
        <table class="border p-3 text-center">
            <thead class="bg-cyan-600 text-white">
              <th class="px-5 py-3 border">Booking ID</th>
              <th class="px-5 py-3 border">Entry</th>
              <th class="px-5 py-3 border">Event</th>
              <th class="px-5 py-3 border">Voucher Name</th>
              <th class="px-5 py-3 border">Hotel</th>
              <th class="px-5 py-3 border">Participants</th>
              <th class="px-5 py-3 border">Booked Date</th>
              <th class="px-5 py-3 border">Appointment Date</th>
            </thead>
      
            <tbody>
              <% bookings.reverse().forEach(book => { %>
              <tr class="hover:shadow-md hover:shadow-cyan-400">
                <td class="px-5 py-3 border"><%= book.book_id %></td>
                <td class="px-5 py-3 border">
                    <%=book.user_nm %>
                    <!-- <%# if(book.member_num == 0) { %> Online <%# } %>  -->
                    <!-- <%# if(book.member_num == 3) { %> Staff <%# } %>  -->
                </td>
                <td class="px-5 py-3 border"><%=book.program %></td>
                <td class="px-5 py-3 border"><%= book.fname %> <%=book.lname %> </td>
                <td class="px-5 py-3 border"><%= book.hotel %> </td>
                <td class="px-5 py-3 border"><%= book.participant_num %> </td>
                <td class="px-5 py-3 border"><%= book.booked_dt %> </td>
                <td class="px-5 py-3 border"><%= book.appointment_dt %> </td>
                
              </tr>
              <% }) %>
            </tbody>
          </table>

          <div class="p-5 bg-[#EEE] font-thin text-center my-10">
            <p>Copyright @ Dolphin Cove JA 2022</p>
          </div>
    </section>
</section>